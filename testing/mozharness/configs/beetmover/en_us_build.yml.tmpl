      s3_key: {{ s3_prefix }}{{ platform }}/{{ locale }}/firefox-{{ version }}.txt
    jsshell:
      artifact: {{ artifact_base_url }}/jsshell-{{ platform }}.zip
      s3_key: {{ s3_prefix }}jsshell-{{ platform }}.zip
    mozharness_package:
      artifact: {{ artifact_base_url }}/mozharness.zip
