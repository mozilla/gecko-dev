Library('winlauncher')

FORCE_STATIC_LIB = True

UNIFIED_SOURCES += [
    'DllBlocklistWin.cpp',
    'ErrorHandler.cpp',
    'LauncherProcessWin.cpp',
    'LaunchUnelevated.cpp',
]

OS_LIBS += [
    'ntdll',
    'oleaut32',
    'ole32',
]

TEST_DIRS += [
    'test',
]

DisableStlWrapping()

if CONFIG['CC_TYPE'] == 'clang-cl':
    AllowCompilerWarnings()  # workaround for bug 1090497
