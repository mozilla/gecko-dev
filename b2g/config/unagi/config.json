    "gaia_l10n_git_root": "https://git.mozilla.org/releases/l10n/%(locale)s/gaia.git",
    "gecko_l10n_git_root": "https://git.mozilla.org/releases/l10n/%(locale)s/gecko.git",
    "gaia": {
        "l10n": {
            "vcs": "hgtool",
            "root": "http://hg.mozilla.org/releases/gaia-l10n/v1_0_1"
        }
    }
