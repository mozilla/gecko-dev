# Suppress warnings in third-party code.
if CONFIG['_MSC_VER']:
    CXXFLAGS += [
        '-wd4005', # macro redefinition
    ]
elif CONFIG['GNU_CXX']:
    CXXFLAGS += [
        '-Wno-unused-local-typedefs',
    ]
