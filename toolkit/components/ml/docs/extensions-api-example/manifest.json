{
  "manifest_version": 2,
  "name": "test-trial-ml",
  "version": "1.0.0",
  "description": "A demo extension that uses the (trial) ML API.",
  "optional_permissions": ["trialML"],
  "permissions": ["<all_urls>", "menus", "scripting", "storage"],
  "icons": {
    "64": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_ui": {
    "page": "settings.html"
  }
}
