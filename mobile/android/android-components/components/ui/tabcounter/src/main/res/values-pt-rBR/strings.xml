<?xml version="1.0" encoding="utf-8"?>
<resources>
  <!--
    Message announced to the user when tab tray is selected
    The %1$s will be replaced by the number of opened tabs
  -->
  <string name="mozac_tab_counter_open_tab_tray">Abas abertas: %1$s. Toque para mudar de aba.</string>
  <!--
    Message announced to the user when tab tray is selected used by a11y services
    The %1$s will be replaced by the number of opened tabs
  -->
  <string name="mozac_open_tab_counter_tab_tray">Abas não privativas abertas: %1$s. Toque para mudar de aba.</string>
  <!--
    Message announced to the user when the tab counter is selected in private browsing mode.
    Used by a11y services. The %1$s will be replaced by the number of opened tabs
  -->
  <string name="mozac_tab_counter_private">Abas privativas abertas: %1$s. Toque para mudar de aba.</string>
  <!-- Browser menu button that creates a new tab -->
  <string name="mozac_browser_menu_new_tab">Nova aba</string>
  <!-- Browser menu button that creates a private tab -->
  <string name="mozac_browser_menu_new_private_tab">Nova aba privativa</string>
  <!-- Browser menu button to close tab. Closes the current session when pressed. -->
  <string name="mozac_close_tab">Fechar aba</string>
  <!-- Menu option to duplicate the current tab -->
  <string name="mozac_ui_tabcounter_duplicate_tab">Duplicar aba</string>
  <!-- Content description of the tab counter toolbar button -->
  <string name="mozac_tab_counter_content_description">O botão contador de abas da barra de ferramentas.</string>
</resources>
