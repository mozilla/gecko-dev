<?xml version="1.0" encoding="utf-8"?>
<resources>
  <!--
    Message announced to the user when tab tray is selected
    The %1$s will be replaced by the number of opened tabs
  -->
  <string name="mozac_tab_counter_open_tab_tray">Варақаҳои кушодашуда: %1$s. Барои гузариш байни варақаҳо зарба занед.</string>
  <!--
    Message announced to the user when tab tray is selected used by a11y services
    The %1$s will be replaced by the number of opened tabs
  -->
  <string name="mozac_open_tab_counter_tab_tray">Варақаҳои ғайрихусусии кушодашуда: %1$s. Барои гузариш байни варақаҳо зарба занед.</string>
  <!--
    Message announced to the user when the tab counter is selected in private browsing mode.
    Used by a11y services. The %1$s will be replaced by the number of opened tabs
  -->
  <string name="mozac_tab_counter_private">Варақаҳои хусусии кушодашуда: %1$s. Барои гузариш байни варақаҳо зарба занед.</string>
  <!-- Browser menu button that creates a new tab -->
  <string name="mozac_browser_menu_new_tab">Варақаи нав</string>
  <!-- Browser menu button that creates a private tab -->
  <string name="mozac_browser_menu_new_private_tab">Варақаи хусусии нав</string>
  <!-- Browser menu button to close tab. Closes the current session when pressed. -->
  <string name="mozac_close_tab">Пӯшидани варақа</string>
  <!-- Menu option to duplicate the current tab -->
  <string name="mozac_ui_tabcounter_duplicate_tab">Такроран кушодани варақа</string>
  <!-- Content description of the tab counter toolbar button -->
  <string name="mozac_tab_counter_content_description">Тугмаи ҳисобкунаки варақаҳо дар навори абзорҳо.</string>
