  <string name="mozac_feature_prompts_suggest_strong_password_2">Використовуйте надійний пароль</string>
  <!-- Title for using the suggest strong password confirmation dialog. %1$s will be replaced with the generated password -->
  <string name="mozac_feature_prompts_suggest_strong_password_message">Використати надійний пароль: %1$s</string>
  <!-- Title for using the suggest strong password confirmation dialog -->
  <string name="mozac_feature_prompts_suggest_strong_password_title">Використати надійний пароль?</string>
  <!-- Content description for the suggest strong password confirmation dialog -->
  <string name="mozac_feature_prompts_suggest_strong_password_description_3">Захистіть свій обліковий запис за допомогою надійного, випадково згенерованого пароля. Його буде збережено у вашому обліковому записі для подальшого використання.</string>
  <!-- Pressing this will use the suggested strong password -->
  <string name="mozac_feature_prompts_suggest_strong_password_use_password">Використати пароль</string>
  <!-- Pressing this will dismiss the suggested strong password dialog -->
  <string name="mozac_feature_prompts_suggest_strong_password_dismiss">Не зараз</string>
  <!-- Title for showing the suggest strong password saved confirmation snackbar -->
  <string name="mozac_feature_prompts_suggest_strong_password_saved_snackbar_title">Пароль збережено</string>
  <!-- Title for showing the suggest strong password updated confirmation snackbar -->
