  <!-- Indicates the add-on is enabled. -->
  <string name="mozac_feature_addons_settings_on">Tàn-án</string>
  <!-- Indicates the add-on is allowed in private browsing mode. -->
  <string name="mozac_feature_addons_settings_allow_in_private_browsing">Fàyè gbàá nínú bíráwúsìnìn ìkọ̀kọ̀</string>
