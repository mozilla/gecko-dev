  <string name="mozac_feature_addons_learn_more">Kọ́ si nípa ìgbaniláàyè</string>
  <!-- The rating of the add-on. -->
  <string name="mozac_feature_addons_rating">Ìṣe òdiwọ̀n</string>
  <!-- The settings of the add-on. -->
