<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Name of the "notification channel" used for displaying download notification. See https://developer.android.com/training/notify-user/channels -->
    <string name="mozac_feature_downloads_notification_channel">Аҭагаларақәа</string>

    <!-- Text shown on the second row of a paused download notification. -->
    <string name="mozac_feature_downloads_paused_notification_text">Аҭагалара аамҭала иаанкылоуп</string>
    <!-- Text shown on the second row of an completed download notification. The filename is shown on the first row. -->
    <string name="mozac_feature_downloads_completed_notification_text2">Аҭагалара хыркәшоуп</string>
    <!-- Text shown on the second row of an failed download notification. The filename is shown on the first row. -->
    <string name="mozac_feature_downloads_failed_notification_text2">Аҭагалара алымшеит</string>

    <!-- Alert dialog confirmation before download a file, this is the title. %1$s will be replaced with the name of the file. -->
    <string name="mozac_feature_downloads_dialog_title2">Аҭагалара (%1$s)</string>
    <!-- Alert dialog confirmation before download a file, this is the positive action. -->
    <string name="mozac_feature_downloads_dialog_download">Иҭагалатәуп</string>
    <!-- Alert dialog confirmation before download a file, this is the negative action. -->
    <string name="mozac_feature_downloads_dialog_cancel">Мап ацәктәуп</string>
    <!-- Error shown when the user is trying to download a invalid file. %1$s will be replaced with the name of the app. -->
    <string name="mozac_feature_downloads_file_not_supported2">%1$s ари афаил хкы аҭагалара ауам</string>

    <!-- Message that appears when the downloaded file could not be opened-->
    <string name="mozac_feature_downloads_could_not_open_file">Афаил аартра алымшеит</string>

    <!-- Message that appears when the downloaded file is a specific type that Android doesn't support opening-->
    <string name="mozac_feature_downloads_open_not_supported1">Афаилқәа %1$s раартра зылшо апликациақәа ыҟамызт</string>

    <!-- Button that pauses the download when pressed -->
    <string name="mozac_feature_downloads_button_pause">Ааанкылара</string>
    <!-- Button that resumes the download when pressed -->
    <string name="mozac_feature_downloads_button_resume">Анаркьаҿ</string>
    <!-- Button that cancels the download when pressed -->
    <string name="mozac_feature_downloads_button_cancel">Мап ацәктәуп</string>
    <!-- Button that opens the downloaded file when pressed -->
    <string name="mozac_feature_downloads_button_open">Аартра</string>
    <!-- Button that restarts the download after a failed attempt -->
    <string name="mozac_feature_downloads_button_try_again">Даҽазнык шәҽазышәшәа</string>

    <!-- Content description for close button -->
    <string name="mozac_feature_downloads_button_close">Иаркуп</string>
    <!-- Title for the third party download app chooser dialog -->
    <string name="mozac_feature_downloads_third_party_app_chooser_dialog_title">Аус хыркәшатәуп ахархәарала</string>
    <!-- Message that appears when trying to download with an external app fails. %1$s will be replaced with the name the external app. -->--&gt;
    <string name="mozac_feature_downloads_unable_to_open_third_party_app">Аартра алымшеит %1$s</string>

    <!-- Text for the info dialog when write to storage permissions have been denied but user tries to download a file. -->
    <string name="mozac_feature_downloads_write_external_storage_permissions_needed_message">Афаилқәеи амедиеи рзинқәа аҭахуп афаилқәа рҭагаларазы. Android асистема архиарақәа рахь ацара, алхра «Азинқәа», нас алхра «Азин аҭара».</string>

    <!-- Alert dialog confirmation before cancelling downloads, this is the title   -->
    <string name="mozac_feature_downloads_cancel_active_downloads_warning_content_title">Хаҭалатәи ачагарақәа ԥыхтәума?</string>
    <!-- Alert dialog confirmation before cancelling private downloads, this is the body. %1$s will be replaced with the name of the file. -->
    <string name="mozac_feature_downloads_cancel_active_private_downloads_warning_content_body">Ахатә ҭаӡҩырақәа зегьы уажәы иауркыр, %1$s ԥыххоит. Ахатә ԥшаара аҟынтәи аҭыҵра уҭахума?</string>
    <!-- Alert dialog confirmation before cancelling downloads, this is the positive action. -->
    <string name="mozac_feature_downloads_cancel_active_downloads_accept">Аҭагалара аԥыхтәуп</string>
    <!-- Alert dialog confirmation before cancelling downloads, this is the negative action. Leaves user in Private browsing -->
    <string name="mozac_feature_downloads_cancel_active_private_downloads_deny">Ахатә ԥшаараҿы аанхара</string>
</resources>
