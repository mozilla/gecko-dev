  const nsITimer = Components.interfaces.nsITimer;

  timer = Components.classes["@mozilla.org/timer;1"].createInstance(nsITimer);
  timer.initWithCallback(function() {
    fontWrite(f);
    response.finish();
  }, query["delay"], nsITimer.TYPE_ONE_SHOT);
}


