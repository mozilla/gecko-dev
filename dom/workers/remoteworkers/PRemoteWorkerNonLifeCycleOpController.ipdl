include RemoteWorkerTypes;
include SharedWorkerOpArgs;

namespace mozilla {
namespace dom{

[ChildProc=any]
protocol PRemoteWorkerNonLifeCycleOpController
{

parent:
  async Terminated();

  async Error(ErrorValue aValue);

child:
  async ExecOp(SharedWorkerOpArgs aOpArgs);

  async Shutdown();
};

} // namespace dom
} // namespace mozilla
